                        android:layout_height="wrap_content"
                        android:fontFamily="@font/verlag_light"
                        android:gravity="right"
                        android:paddingTop="8dp"
                        android:text="90258"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/verlag_light"
                        android:gravity="right"
                        android:paddingTop="8dp"
                        android:text="JBCZHF3"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/verlag_light"
                        android:gravity="right"
                        android:paddingTop="8dp"
                        android:text="JBPR"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/verlag_light"
                        android:gravity="right"
                        android:paddingTop="8dp"
                        android:text="JBPRE"
                        android:textColor="@color/black"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/textView20"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/verlag_light"
                        android:gravity="right"
                        android:paddingTop="8dp"
                        android:text="PNWU8O0BLT17BBV61Y18"
                        android:textColor="@color/black"
                        android:textSize="16sp" />
                </LinearLayout>

            </LinearLayout>

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:src="@drawable/ic_content" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/verlag_bold"
                android:text="Support"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtStructureProduct4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/verlag_light"
                android:paddingTop="8dp"
                android:text="@string/contact_support"
                android:textColor="@color/black"
                android:textSize="16sp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="16dp"
                android:src="@drawable/ic_content" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/verlag_bold"
                android:text="About"
                android:textColor="@color/black"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/txtStructureProduct5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/verlag_light"
                android:paddingTop="8dp"
                android:textColor="@color/black"
                android:textSize="16sp" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>
